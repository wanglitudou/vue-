  methods: {
    activStarty() {
      this.$axios
        .post(window.ajaxSrc + "/client/message/view", {
          id: this.$route.query.activeId
        })
        .then(res => {
          this.title = res.data.data.title;
          this.content = res.data.data.content;
        });
    },
    TptbakClick() {
      this.$router.go(-1);
    }
  }
};
</script>
