          <input class=\"inp_addinp\" v-model=\"form.email\" type=\"text\" placeholder=\"请输入邮箱\">\r\n        </p>\r\n      </div>\r\n      <div class=\"rejiggeradd\">\r\n        <div class=\"zhecent\" v-if=\"dialogproencrypt\" @click=\"encryptshurt()\"></div>\r\n        <div class=\"translate\" v-if=\"dialogproencrypt\">\r\n          <div class=\"hello\">\r\n            <div class=\"btn-group\">\r\n              <ul>\r\n                <li\r\n                  class=\"addtents\"\r\n                  :class=\"[acquisitionadd,acquisitionId == item.id ? acquisitionActive : '']\"\r\n                  v-for=\"(item,index) in provinceList\"\r\n                  v-on:click=\"proencryptClick(item.id,item.name)\"\r\n                  :key=\"index.item\"\r\n                >{{item.name}}</li>\r\n              </ul>\r\n            </div>\r\n            <div class=\"wrap\">\r\n              <ul>\r\n                <li\r\n                  class=\"propagate\"\r\n                  :class=\"[unterweltadd,unterweltId == item.id ? unterweltActive : '']\"\r\n                  v-for=\"(item,index) in citylist\"\r\n                  @click=\"cityClick(item.id,item.name)\"\r\n                  :key=\"item.index\"\r\n                >{{item.name}}</li>\r\n              </ul>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <p class=\"name_cents\">\r\n          <span>地址</span>\r\n          <span>\r\n            <span>{{pro_name}}</span>\r\n            <span>{{city_name}}</span>\r\n            <span class=\"compileradd\" @click=\"toSelectClick()\">编辑</span>\r\n          </span>\r\n        </p>\r\n      </div>\r\n      <div class=\"hold\">\r\n        <button class=\"keep\" @click=\"toConservation()\">保存</button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport { Toast, Popup, Picker, Indicator } from \"mint-ui\";\r\nimport \"mint-ui/lib/style.css\";\r\nimport { imgPreview } from \"../../../static/imgSend.js\";\r\nexport default {\r\n  components: {\r\n    \"mt-picker\": Picker,\r\n    \"mt-popup\": Popup\r\n    // VuePickers\r\n  },\r\n  data() {\r\n    return {\r\n      acquisitionadd: \"acquisitionadd\",\r\n      acquisitionId: \"acquisitionId\",\r\n      acquisitionActive: \"acquisition_Active\",\r\n      unterweltadd: \"unterweltadd\",\r\n      unterweltId: \"unterweltId\",\r\n      unterweltActive: \"unterwelt_Active\",\r\n      gender: \"male\",\r\n      genders: [\r\n        {\r\n          name: \"男\",\r\n          gender: \"male\"\r\n        },\r\n        {\r\n          name: \"女\",\r\n          gender: \"female\"\r\n        }\r\n      ],\r\n      type_sex: \"\",\r\n      nickname: \"\",\r\n      // sex: \"1\",\r\n      data: {\r\n        avatar: \"\"\r\n      },\r\n      // flagSex: \"\",\r\n      proId: \"\",\r\n      cityId: \"\",\r\n      proName: \"\",\r\n      id: \"\",\r\n      dialogproencrypt: false,\r\n      dialogsexual: false,\r\n      provinceList: [], //省\r\n      citylist: [], //市\r\n      listArr: [],\r\n      sexuallist: [{ name: \"男\", id: \"1\" }, { name: \"女\", id: \"2\" }],\r\n      pro_type: \"\",\r\n      pro_name: \"\",\r\n      city_type: \"\",\r\n      city_name: \"\",\r\n      user_name: \"\",\r\n      sex_name: \"\",\r\n      sex_type: \"\",\r\n      addressPickerShow: false,\r\n      show1: false,\r\n      show: false,\r\n      //addressSlots:centprolist,\r\n      form: {\r\n        fileList: [],\r\n        avatar: null, //头像\r\n        user_name: \"\", //用户名\r\n        sex: \"\", //性别 1男 2 女\r\n        email: \"\", //邮箱\r\n        mobile: \"\", //手机号\r\n        // province: \"\", //\t省id\r\n        // city: \"\", //\t市id\r\n        token: \"\"\r\n      }\r\n    };\r\n  },\r\n  mounted() {\r\n    $(\".starandnote\").click(function() {\r\n      $(\".starandnote\")\r\n        .find(\"b\")\r\n        .removeClass(\"activess\");\r\n      $(this)\r\n        .find(\"b\")\r\n        .addClass(\"activess\");\r\n    });\r\n  },\r\n  computed: {},\r\n  created(e) {\r\n    // this.pro_type = 1;\r\n    // alert(this.pro_type)\r\n    var ua = navigator.userAgent.toLowerCase();\r\n    //判断是否是苹果手机，是则是true\r\n    var isIos = ua.indexOf(\"iphone\") != -1 || ua.indexOf(\"ipad\") != -1;\r\n    if (isIos) {\r\n      $(\"input:file\").removeAttr(\"capture\");\r\n    }\r\n    Indicator.open();\r\n    setTimeout(() => {\r\n      Indicator.close();\r\n    }, 500);\r\n    toPersonalDetails: {\r\n      console.log(\"设置\");\r\n      let getuser_type = localStorage.getItem(\"user_type\");\r\n      let that = this;\r\n      let retoken = localStorage.getItem(\"token\");\r\n      console.log(retoken + \"44444\");\r\n      that.$axios\r\n        .post(window.ajaxSrc + \"/client/user/getinfo\", {\r\n          token: retoken\r\n        })\r\n        .then(res => {\r\n          that.form.user_name = res.data.data.user_name;\r\n          localStorage.setItem(\"nickname\", that.form.user_name);\r\n          that.sex = res.data.data.sex;\r\n          that.form.mobile = res.data.data.mobile;\r\n          that.form.email = res.data.data.email;\r\n          that.form.avatar = res.data.data.avatar;\r\n          localStorage.setItem(\"avatar\", res.data.data.avatar);\r\n          that.city_name = res.data.data.city_name;\r\n          that.pro_name = res.data.data.province_name;\r\n          that.pro_type = res.data.data.province;\r\n          that.city_type = res.data.data.city;\r\n          let gender = \"male\"; //默认男\r\n          if (res.data.data.sex == 2) {\r\n            gender = \"female\"; //女\r\n          }\r\n          that.gender = gender;\r\n          console.log(\"lailaili\");\r\n          console.log(res);\r\n          console.log(that.gender);\r\n        });\r\n    }\r\n  },\r\n  methods: {\r\n    //性别选择\r\n    convincedClick() {\r\n      // alert(1);\r\n      // console.log(e.target.className);\r\n      // console.log($(this));\r\n      // $('.starandnote span').removeClass(\"active\");\r\n      // $(this).addClass(\"active\");\r\n      // if(flagSex == 1){\r\n      //   this.man = '../../assets/icon/starnoteadd.png';\r\n      //   flagSex = 2;\r\n      // }\r\n      // if(flagSex == 2){\r\n      //   this.man = '../../assets/icon/starnoteadd.png';\r\n      //   flagSex = 2;\r\n      // }\r\n    },\r\n    // confidenceClick(e) {\r\n    //   alert(e);\r\n    //  $(this).removeClass(\"sex_fix\");\r\n    //   $(this).addClass(\"sex_sents\");\r\n    // },\r\n    // onValuesChange(picker, values) {\r\n    //   console.log(\"123\");\r\n    //   console.log(picker);\r\n    //   if (values[0] > values[1]) {\r\n    //     picker.setSlotValue(1, values[0].name);\r\n    //   }\r\n    // },\r\n    // close() {\r\n    //   this.show1 = false;\r\n    //   this.show = false;\r\n    // },\r\n    //性别\r\n    // sexualClick() {\r\n    //   this.dialogsexual = true;\r\n    // },\r\n    // windonClick(num, str) {\r\n    //   console.log(num);\r\n    //   console.log(str);\r\n    //   this.sex_type = num || \"\";\r\n    //   this.sex_name = str || \"\";\r\n    //   console.log(\"num+str\");\r\n    // },\r\n    // sexualshurt() {\r\n    //   this.dialogsexual = false;\r\n    // },\r\n    //获取省\r\n    toSelectClick() {\r\n      this.dialogproencrypt = true;\r\n      this.$axios\r\n        .post(window.ajaxSrc + \"/client/index/area\", {\r\n          id: this.proId,\r\n          name: this.proName\r\n        })\r\n        .then(res => {\r\n          if (res.status == 200) {\r\n            this.provinceList = res.data.data;\r\n          }\r\n        });\r\n      console.log(\"选择地址\");\r\n    },\r\n    encryptshurt() {\r\n      this.dialogproencrypt = false;\r\n    },\r\n    // 获取市\r\n    proencryptClick(num, str) {\r\n      this.acquisitionId = num;\r\n      this.pro_type = num || \"\";\r\n      this.pro_name = str || \"\";\r\n      console.log(num);\r\n      console.log(str);\r\n      localStorage.setItem(\"pro_name\", this.pro_name);\r\n      //如果只选择省 那么市就清空 只显示省\r\n      this.city_name = \"\";\r\n      this.$axios\r\n        .post(window.ajaxSrc + \"/client/index/city\", {\r\n          province: num\r\n        })\r\n        .then(res => {\r\n          if (res.status == 200) {\r\n            this.citylist = res.data.data;\r\n            console.log(\"city类\");\r\n            console.log(this.citylist);\r\n          }\r\n        });\r\n    },\r\n    cityClick(num, str) {\r\n      this.unterweltId = num;\r\n      this.city_type = num || \"\";\r\n      this.city_name = str || \"\";\r\n      console.log(\"这里\");\r\n      console.log(this.city_type);\r\n      localStorage.setItem(\"city_name\", this.city_name);\r\n      console.log(this.city_name);\r\n      console.log(\".........\");\r\n      this.dialogproencrypt = false;\r\n    },\r\n    toConservation() {\r\n      let that = this;\r\n      if (that.form.avatar == \"\") {\r\n        Toast(\"请上传图片\");\r\n        return false;\r\n      }\r\n      if (that.form.user_name == \"\" || that.form.user_name == \"null\") {\r\n        Toast(\"姓名不能为空\");\r\n        return false;\r\n      }\r\n      //验证姓名\r\n      if (!/^([a-zA-Z0-9\\u4e00-\\u9fa5\\·]{1,10})$/.test(that.form.user_name)) {\r\n        Toast(\"请输入正确的姓名格式\");\r\n        return false;\r\n      }\r\n      //验证手机号\r\n      if (!/^1[3|4|5|7|8][0-9]\\d{4,8}$/.test(that.form.mobile)) {\r\n        Toast(\"请输入正确的手机号格式\");\r\n        return false;\r\n      }\r\n      //验证邮箱\r\n      if (that.form.email != \"\") {\r\n        if (\r\n          !/^[a-zA-Z0-9_-]+@([a-zA-Z0-9]+\\.)+(com|cn|net|org)$/.test(\r\n            that.form.email\r\n          )\r\n        ) {\r\n          Toast(\"请输入正确的邮箱格式\");\r\n          return false;\r\n        }\r\n        // Toast(\"邮箱不能为空\");\r\n        // return false;\r\n      }\r\n\r\n      // if (that.form.email == \"\") {\r\n      //   Toast(\"邮箱不能为空\");\r\n      //   return false;\r\n      // }\r\n      // if (\r\n      //   !/^[a-zA-Z0-9_-]+@([a-zA-Z0-9]+\\.)+(com|cn|net|org)$/.test(\r\n      //     that.form.email\r\n      //   )\r\n      // ) {\r\n      //   Toast(\"请输入